@font-face
  font-family: 'icomoon'
  src:  url('../../assets/fonts/icomoon.eot?xh1v14')
  src:  url('../../assets/fonts/icomoon.eot?xh1v14#iefix') format('embedded-opentype'),
    url('../../assets/fonts/icomoon.ttf?xh1v14') format('truetype'),
    url('../../assets/fonts/icomoon.woff?xh1v14') format('woff'),
    url('../../assets/fonts/icomoon.svg?xh1v14#icomoon') format('svg')
  font-weight: normal
  font-style: normal


[class^="icon-"], [class*=" icon-"]
  /* use !important to prevent issues with browser extensions that change fonts */
  font-family: 'icomoon' !important
  speak: none
  font-style: normal
  font-weight: normal
  font-variant: normal
  text-transform: none
  line-height: 1

  /* Better Font Rendering =========== */
  -webkit-font-smoothing: antialiased
  -moz-osx-font-smoothing: grayscale


.icon-home:before
  content: "\e900"

.icon-download3:before
  content: "\e9c7"

.icon-plus:before
  content: "\ea0a"

.icon-new-tab:before
  content: "\ea7e"

.icon-share2:before
  content: "\ea82"

